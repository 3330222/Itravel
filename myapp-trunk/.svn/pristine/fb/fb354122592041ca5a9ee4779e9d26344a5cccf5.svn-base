package com.myrent.vo;

import com.myrent.data.model.vo.BaseVO;
import com.myrent.vo.user.UserVO;

@SuppressWarnings("serial")
public class PaymentVO extends BaseVO {
	private String paymentId;
	private Double cTime;
	private String paidAmt;
	private String paymentStatus;
	private String uTime;
	private String userId;
	private String paymentType;
	private String orderId;
	private UserVO user;
	private OrderVO order;
	private RefundVO refund;
	public String getPaymentId() {
		return paymentId;
	}
	public void setPaymentId(String paymentId) {
		this.paymentId = paymentId;
	}
	public Double getcTime() {
		return cTime;
	}
	public void setcTime(Double cTime) {
		this.cTime = cTime;
	}
	public String getPaidAmt() {
		return paidAmt;
	}
	public void setPaidAmt(String paidAmt) {
		this.paidAmt = paidAmt;
	}
	public String getPaymentStatus() {
		return paymentStatus;
	}
	public void setPaymentStatus(String paymentStatus) {
		this.paymentStatus = paymentStatus;
	}
	public String getuTime() {
		return uTime;
	}
	public void setuTime(String uTime) {
		this.uTime = uTime;
	}
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public String getPaymentType() {
		return paymentType;
	}
	public void setPaymentType(String paymentType) {
		this.paymentType = paymentType;
	}
	public String getOrderId() {
		return orderId;
	}
	public void setOrderId(String orderId) {
		this.orderId = orderId;
	}
	public UserVO getUser() {
		return user;
	}
	public void setUser(UserVO user) {
		this.user = user;
	}
	public OrderVO getOrder() {
		return order;
	}
	public void setOrder(OrderVO order) {
		this.order = order;
	}
	public RefundVO getRefund() {
		return refund;
	}
	public void setRefund(RefundVO refund) {
		this.refund = refund;
	}
	

}
